<h2>Customer Listing</h2>
<% if (allrecs.length > 0) {%>
    <table border="1">
        <tr>
            <th>CustomerID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>City</th>
            <th>State</th>
        </tr>
    <% allrecs.forEach((recref, index) => { %>
        <tr>
            <td><%= recref.customer_id %></td>
            <td><%= recref.firstname %></td>
            <td><%= recref.lastname %></td>
            <td><%= recref.email %></td>
            <td><%= recref.phone %></td>
            <td><%= recref.city %></td>
            <td><%= recref.state %></td>
        </tr>
    <% }); %>
        </table>
    <% } else { %>
        <p>Not Records Available</p>
    <% } %>