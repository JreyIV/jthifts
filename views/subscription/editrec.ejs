<h1> Edit</h1>
<form action="/subscription/save" method="post">
    <input type="hidden" name="auto_id" value="<%= onerec.subscription_id %>" >
<table>
    <tr> <td> Subscription ID: </td> <td> <input type="text" name="subscription_id" value="<%= onerec.subscription_id %>" required> </td> </tr>
    <tr> <td> Customer ID: </td> <td> <input type="text" name="customer_id" value="<%= onerec.customer_id %>" required> </td> </tr>
    <tr> <td> Subscribe Date: </td> <td> <input type="date" name="subscribedate" value="<%= onerec.subscribedate %>" required> </td> </tr>
    <tr> <td> unubscribe Date: </td> <td> <input type="date" name="unsubscribedate" value="<%= onerec.unsubscribedate %>" required> </td> </tr>
</table>
<button type="submit">Save</button>
</form>